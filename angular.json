{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "bigtransfer": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/bigtransfer",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": false,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
			  "src/assets/fonts/icomoon/style.css",
			   "src/assets/css/bootstrap.min.css",
			    "src/assets/css/jquery-ui.css",
				 "src/assets/css/owl.carousel.min.css",
				  "src/assets/css/owl.theme.default.min.css",
		 
				    "src/assets/css/jquery.fancybox.min.css",
					 "src/assets/css/bootstrap-datepicker.css",
					  "src/assets/fonts/flaticon/font/flaticon.css",
					   "src/assets/css/aos.css",
					    "src/assets/css/style.css" 
			  
            ],
            "scripts": [
			"src/assets/js/jquery-3.3.1.min.js",
			"src/assets/js/jquery-migrate-3.0.1.min.js",
			"src/assets/js/jquery-ui.js",
			"src/assets/js/popper.min.js",
			"src/assets/js/bootstrap.min.js",
			"src/assets/js/owl.carousel.min.js",
			"src/assets/js/jquery.stellar.min.js",
			"src/assets/js/jquery.countdown.min.js",
			"src/assets/js/bootstrap-datepicker.min.js",
			"src/assets/js/jquery.easing.1.3.js",
			"src/assets/js/aos.js",
			"src/assets/js/jquery.fancybox.min.js",
			"src/assets/js/jquery.sticky.js",
			"src/assets/js/main.js"
			
			]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "bigtransfer:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "bigtransfer:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "bigtransfer:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "bigtransfer:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "bigtransfer:serve:production"
            }
          }
        }
      }
    }},
  "defaultProject": "bigtransfer"
}